{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*",
            "/css/*",
            "/js/*",
            "/api/*",
            "/*.{png,jpg,gif,svg,css,js,ico,woff,woff2,ttf,eot}"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    }
}